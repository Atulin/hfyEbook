{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"vcs": {
		"clientKind": "git",
		"enabled": true,
		"defaultBranch": "master"
	},
	"files": {
		"ignore": ["cache", "output", "templates"]
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
